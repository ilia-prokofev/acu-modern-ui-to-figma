{
    "manifest_version": 3,
    "name": "Figmatica",
    "version": "1.0.0",
    "description": "Chrome extension to export HTML content to Figma",
    "action": {
        "default_popup": "popup.html"
    },
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
    "permissions": ["scripting", "activeTab", "clipboardWrite"],
    "host_permissions": ["<all_urls>"]
}