{
    "manifest_version": 3,
    "name": "Modern UI to Figma Exporter",
    "version": "1.0.0",
    "description": "Parses HTML and copies it to clipboard for Figma",
    "action": {
        "default_popup": "popup.html"
    },
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
    "permissions": ["scripting", "activeTab", "clipboardWrite"]
}